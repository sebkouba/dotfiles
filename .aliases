# mv, rm, cp
alias mv='mv -v'
alias rm='rm -i -v'
alias cp='cp -v'

alias ls='ls -lAh'

alias dps='docker ps'
alias dpsa='docker ps -a'

alias grep='grep --color=auto'
  
# auto create parent directories
alias mkdir='mkdir -pv'
  
# stop after 3 pings
alias ping='ping -c 3'

alias mtmux="tmux new-session -A -s main"

# fasd vim alias
alias v='f -e vim'
